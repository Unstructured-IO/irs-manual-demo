  },
  {
    "element_id": "2a1ffc8beac41c488e177f51f01415ef",
    "text": "PN-CLOSING-CD (",
    "type": "ListItem",
    "metadata": {
      "filename": "../zips/irm02-002-008--2022-10-19.pdf",
      "page_number": 7
    }
  },
  {
    "element_id": "8159ee1a2d98fae4f7aecc8a5b835c58",
    "text": ") When all lines",
    "type": "ListItem",
    "metadata": {
      "filename": "../zips/irm02-002-008--2022-10-19.pdf",
      "page_number": 7
    }
  },
  {
    "element_id": "3973e022e93220f9212c18d0d0c543ae",
    "text": "-",
    "type": "ListItem",
