  {
    "element_id": "553e873f123a65c2890d91e8d9f784aa",
    "text": "Note: TC 520 cc 71 - 74 are cases generally assigned to either Exam or Counsel.",
    "type": "NarrativeText",
    "metadata": {
      "filename": "../zips/irm05-012-011--2013-10-14.pdf",
